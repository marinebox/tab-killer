{
    "name": "tab-killer",
    "description": "Close all duplicate tabs.",
    "version": "0.1",
    "manifest_version": 2,

    "browser_action": {
        "default_title": "tab-killer"
    },

    "permissions": ["tabs"],

    "background": {
        "scripts": ["background.js"],
        "persistent": false
    }
}