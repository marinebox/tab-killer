{
  "name": "tab-killer",
  "description": "Close all duplicate tabs.",
  "version": "1.0.0",
  "manifest_version": 3,

  "action": {
    "default_title": "tab-killer",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs", "storage"],

  "icons": {
    "16": "images/16icon.png",
    "48": "images/48icon.png",
    "128": "images/128icon.png"
  }
}
